{
  "name": "neurokinetics-ai",
  "version": "1.0.0",
  "description": "Clinical-grade mobile/tablet autism screening and intervention platform",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace NeuroKineticsAI.xcworkspace -scheme NeuroKineticsAI -configuration Release -destination generic/platform=iOS -archivePath NeuroKineticsAI.xcarchive archive"
  },
  "dependencies": {
